{
  "name": "@metrichor/epi2me-api",
  "version": "3.0.1839",
  "license": "MPL-2.0",
  "repository": "git@github.com:nanoporetech/epi2me-api.git",
  "description": "API for communicating with the EPI2ME website(s)",
  "main": "dist/index.js",
  "types": "dist/src/index.d.ts",
  "module": "dist/index.es.js",
  "dependencies": {
    "@apollo/client": "^3.1.3",
    "@lifeomic/axios-fetch": "^1.4.2",
    "@types/lifeomic__axios-fetch": "^1.4.0",
    "@types/socket.io-client": "^1.4.32",
    "aws-sdk": "^2.610.0",
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "cross-fetch": "^3.0.5",
    "fdir": "^3.4.3",
    "fs-extra": "^8.1.0",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.1",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.19",
    "proxy-agent": "^3.1.1",
    "rxjs": "^6.6.2",
    "save": "^2.4.0",
    "socket.io-client": "^2.3.0",
    "sqlite": "^3.0.3",
    "tunnel": "^0.0.6"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@types/bunyan": "^1.8.6",
    "@types/fs-extra": "^8.1.0",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.24",
    "@types/rollup": "^0.54.0",
    "@types/rollup-plugin-json": "^3.0.2",
    "@types/tunnel": "0.0.1",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "bunyan": "^1.8.12",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.4",
    "mocha": "^8.1.1",
    "mock-fs": "^4.11.0",
    "nyc": "^15.0.0",
    "prettier": "^2.0.5",
    "rollup": "^2.22.1",
    "rollup-plugin-analyzer": "^3.2.1",
    "rollup-plugin-cpy": "^2.0.1",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-generate-package-json": "^3.1.3",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-license": "^2.2.0",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "sinon": "^8.1.1",
    "tmp": "^0.1.0",
    "ts-node": "^8.6.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.7",
    "xunit-file": "*"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run lint:fix",
      "git add --force"
    ],
    "*.{json,md,graphql}": [
      "prettier --write",
      "git add --force"
    ]
  },
  "scripts": {
    "build:version": "jq \".version=\\\"$(jq -r .version package.json | cut -d . -f 1-2).${PATCH:-$(date +%-H%M)}\\\"\" < package.json > package.json.tmp && mv package.json.tmp package.json",
    "lint:source": "eslint '*/**/*.{js,ts,tsx}' --ignore-path .eslintignore --ignore-pattern \"!**/.*\"",
    "lint:quiet": "npm run lint:source -- --quiet",
    "lint:warn": "npm run lint:source",
    "lint:fix": "npm run lint:quiet -- --fix",
    "lint": "npm run lint:quiet",
    "deps": "npm ci",
    "clean:dist": "rm -rf dist",
    "clean:build": "rm -rf build && rm -rf dist/lib",
    "clean": "npm run clean:build && npm run clean:dist",
    "test": "mocha --recursive --require ts-node/register test/**/*.{js,ts}",
    "test:single": "mocha --require ts-node/register test/**/*.{js,ts}",
    "cover": "npm install && npm run lint && npx nyc --reporter=html --reporter=text mocha --recursive --require ts-node/register test",
    "build": "npm ci && npm run build:dist",
    "rollup:build": "npx rollup -c",
    "rollup:watch": "npx rollup -cw",
    "build:light": "npm run clean:dist && npm run rollup:build",
    "build:dist": "npm run build:version && npm run clean:dist && npm ci && npm run rollup:build",
    "ts:defs": "tsc --outDir dist --declarationDir dist/types --emitDeclarationOnly"
  }
}
